<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="pstc_spmlv2_async" targetNamespace="urn:oasis:names:tc:SPML:2:0:async" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
