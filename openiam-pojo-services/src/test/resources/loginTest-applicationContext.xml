	<bean id="loginAttrBean" class="org.openiam.idm.srvc.auth.dto.LoginAttribute">
		<property name="loginAttrId" value="100" />
		<property name="login" ref="loginBean2" />
		<property name="name" value="ATTR1" />
		<property name="value" value="100" />
	</bean>
-->
</beans>


