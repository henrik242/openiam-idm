<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
    					http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
    					http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd">




	<!-- User Test Beans -->
 
	<bean id="grpBean" class="org.openiam.idm.srvc.grp.dto.Group" >
		<property name="createdBy" value="100"/>
		<property name="grpName" value="Base Group"/>
		<property name="inheritFromParent" value="false"/>
	</bean>

	<bean id="grpBean2" class="org.openiam.idm.srvc.grp.dto.Group" >
		<property name="createdBy" value="101"/>
		<property name="grpName" value="Sales Group"/>
		<property name="inheritFromParent" value="true"/>
		<property name="parentGrpId" value="BASE_GRP"/>
	</bean>
	<bean id="grpBean3" class="org.openiam.idm.srvc.grp.dto.Group" >
		<property name="createdBy" value="102"/>
		<property name="grpName" value="Finance Group"/>
		<property name="inheritFromParent" value="true"/>
		<property name="parentGrpId" value="BASE_GRP"/>
	</bean>

	<bean id="grpBean4" class="org.openiam.idm.srvc.grp.dto.Group" >
		<property name="createdBy" value="102"/>
		<property name="grpName" value="Finance Sub Group"/>
		<property name="inheritFromParent" value="true"/>
		<property name="parentGrpId" value="Finance_GRP"/>
	</bean>
	<bean id="grpBean5" class="org.openiam.idm.srvc.grp.dto.Group" >
		<property name="createdBy" value="102"/>
		<property name="grpName" value="Finance Sub Group2"/>
		<property name="inheritFromParent" value="true"/>
		<property name="parentGrpId" value="Finance_GRP"/>
	</bean>

	<bean id="grpBean6" class="org.openiam.idm.srvc.grp.dto.Group" >
		<property name="createdBy" value="102"/>
		<property name="grpName" value="Finance cgs Group"/>
		<property name="inheritFromParent" value="true"/>
		<property name="parentGrpId" value="Finance_sub_GRP"/>
	</bean>

	<bean id="grpBean7" class="org.openiam.idm.srvc.grp.dto.Group" >
		<property name="createdBy" value="102"/>
		<property name="grpName" value="Finance sf Group"/>
		<property name="inheritFromParent" value="true"/>
		<property name="parentGrpId" value="Finance_sub_GRP"/>
	</bean>


	<bean id="grpAttribute" class="org.openiam.idm.srvc.grp.dto.GroupAttribute" >
		<property name="name" value="SSN"/>
		<property name="value" value="11111122"/>	
	</bean>
	<bean id="grpAttribute2" class="org.openiam.idm.srvc.grp.dto.GroupAttribute" >
		<property name="name" value="City"/>
		<property name="value" value="Cortlandt"/>	
	</bean>
	<bean id="grpAttribute3" class="org.openiam.idm.srvc.grp.dto.GroupAttribute" >
		<property name="name" value="state"/>
		<property name="value" value="NY"/>	
	</bean>

	<bean id="roleBean" class="org.openiam.idm.srvc.role.dto.Role" >
		<property name="id" ref="roleId"/>
		<property name="roleName" value="TEST ROLE"/>
		<property name="description" value="Testing role features"/>	
	</bean>
	
	<bean id="roleId" class="org.openiam.idm.srvc.role.dto.RoleId">
		<property name="roleId" value="TESTROLE" />
		<property name="serviceId" value="IDM" />
	</bean>
	
	
</beans>


