    <dependency>
      <groupId>org.openiam</groupId>
      <artifactId>openiam-pojo-intf</artifactId>
      <version>2.1.0</version>
    </dependency>
    <dependency>
      <groupId>org.openiam</groupId>
      <artifactId>idm-connector-intf</artifactId>
      <version>2.1.0</version>
    </dependency>
    
    <dependency>
      <groupId>org.openiam</groupId>
      <artifactId>openiam-pojo-services</artifactId>
      <version>2.1.0</version>
    </dependency>
    
    
    <dependency>
    	<groupId>asm</groupId>
