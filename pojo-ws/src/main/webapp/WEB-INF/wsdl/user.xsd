	<import namespace="urn:idm.openiam.org/srvc/meta/dto" schemaLocation="metadata.xsd"/>
	<complexType name="userAttribute">
		<sequence>
			<element name="id" type="string" minOccurs="0"/>
			<element name="metadataId" type="string" minOccurs="0"/>
			<element name="name" type="string" minOccurs="0"/>
			<element name="userId" type="string" minOccurs="0"/>
			<element name="value" type="string" minOccurs="0"/>
		</sequence>
	</complexType>
	<complexType name="userAttributeMap">
		<sequence>
			<element name="userAttributeEntry" minOccurs="0" maxOccurs="unbounded">
				<complexType>
					<sequence>
						<element name="userAttribute" type="tns:userAttribute" minOccurs="0"/>
					</sequence>
					<attribute name="key" type="string"/>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="userAttributeSet">
		<sequence>
			<element name="userAttributeObj" minOccurs="0" maxOccurs="unbounded">
