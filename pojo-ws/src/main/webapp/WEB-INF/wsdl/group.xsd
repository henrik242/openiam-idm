<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="urn:idm.openiam.org/srvc/grp/dto" xmlns:meta="urn:idm.openiam.org/srvc/meta/dto" xmlns:user="urn:idm.openiam.org/srvc/user/dto" xmlns:role="urn:idm.openiam.org/srvc/role/dto" targetNamespace="urn:idm.openiam.org/srvc/grp/dto" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<import namespace="urn:idm.openiam.org/srvc/role/dto" schemaLocation="role.xsd"/>
	<import namespace="urn:idm.openiam.org/srvc/user/dto" schemaLocation="user.xsd"/>
	<import namespace="urn:idm.openiam.org/srvc/meta/dto" schemaLocation="metadata.xsd"/>
	<complexType name="group">
		<sequence>
			<element name="roles" type="role:roleSet" minOccurs="0"/>
			<element name="attributes" type="tns:groupAttributeMap" minOccurs="0"/>
			<element name="users" type="user:userSet" minOccurs="0"/>
